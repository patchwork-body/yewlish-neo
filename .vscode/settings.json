{
  "cSpell.words": [
    "attributify",
    "Avaya",
    "bindgen",
    "brix",
    "describedby",
    "hashchange",
    "labelledby",
    "onchange",
    "onclick",
    "rustc",
    "yewlish"
  ],
  "rust-analyzer.diagnostics.disabled": [
    "unresolved-import"

  ],
  "rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": true,
  "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
  "rust-analyzer.inlayHints.reborrowHints.enable": "never",
  "rust-analyzer.semanticHighlighting.punctuation.enable": true,
  "rust-analyzer.diagnostics.styleLints.enable": true,
  "rust-analyzer.check.extraArgs": [
    "--",
    "-W",
    "clippy::needless-pass-by-value",
    "-W",
    "clippy::pedantic",
    "-A",
    "clippy::module-name-repetitions",
    "-A",
    "clippy::cast-lossless",
    "-A",
    "clippy::wildcard-imports",
    "-A",
    "clippy::struct-field-names",
    "-A",
    "clippy::no-effect-underscore-bindings",
    "-A",
    "clippy::missing-docs-in-private-items",
    "-A",
    "clippy::missing-docs-in-private-impls",
    "-A",
    "clippy::missing-docs-in-private-trait-impls",
    "-A",
    "clippy::missing-docs-in-private-extern-crate",
    "-A",
    "clippy::missing-docs-in-private-use",
    "-A",
    "clippy::missing-errors-doc",
    "-A",
    "clippy::similar-names",
    "-A",
    "clippy::cast-possible-truncation",
    "-A",
    "clippy::cast-sign-loss",
    "-A",
    "clippy::cast-possible-loss",
  ],
  "editor.fontLigatures": true,
}